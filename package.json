		"passport-local": "^1.0.0",
		"reflect-metadata": "^0.2.0",
		"rxjs": "^7.8.1",
		"sharp": "^0.33.4"
	},
	"devDependencies": {
		"@eslint/js": "^9.2.0",
		"@nestjs/cli": "^10.0.0",
		"@nestjs/schematics": "^10.0.0",
		"@nestjs/testing": "^10.0.0",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.2",
		"@types/multer": "^1.4.11",
		"@types/node": "^20.12.8",
		"@types/supertest": "^6.0.0",
		"eslint": "^8.57.0",
